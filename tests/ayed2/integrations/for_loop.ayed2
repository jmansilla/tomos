var i: int
var asc: array[5] of int
var desc: array[5] of int
var sum: array[5] of int

for i := 0 to 4 do
    asc[i] := i
    sum[i] := i
od

for i := 4 downto 0 do
    desc[i] := 4 - i
    sum[i] := sum[i] + (4 - i)
od

